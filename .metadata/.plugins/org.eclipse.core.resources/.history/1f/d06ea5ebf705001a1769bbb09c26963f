import se.lth.cs.pt.window.SimpleWindow;

public class SampleWindowExample {
	public static void main(String[] args) {
		SimpleWindow w = new SimpleWindow(1024, 1024, "Drawing Window");
		w.moveTo(100, 100);
		w.lineTo(150, 100);
		w.moveTo(w.getWidth()/2, w.getHeight()/2);
		w.writeText("Hej v√§rlden!");
	}
}
